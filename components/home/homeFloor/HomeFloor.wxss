.home-floor .home-floor-group {
  padding: 10rpx;
}
.home-floor .home-floor-group .home-floor-title {
  width: 100%;
}
.home-floor .home-floor-group .home-floor-title image {
  width: 100%;
}
.home-floor .home-floor-group .home-floor-content {
  overflow: hidden;
}
.home-floor .home-floor-group .home-floor-content navigator {
  float: left;
  width: 33.33%;
}
.home-floor .home-floor-group .home-floor-content navigator:nth-last-child(-n+4) {
  height: 27.07711207vw;
  border-left: 10rpx solid #fff;
}
.home-floor .home-floor-group .home-floor-content navigator:nth-last-child(-n+2) {
  border-top: 10rpx solid #fff;
}
.home-floor .home-floor-group .home-floor-content navigator image {
  width: 100%;
  height: 100%;
}
